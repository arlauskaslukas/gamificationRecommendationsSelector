        <ns0:outputEntry id="LiteralExpression_0vkovvg">
          <ns0:text>"Vizualiai išskirti dar neturimus pasiekimus, pateikti nuorodas į užduotis, kurios padeda jų siekti, kad naudotojas galėtų atrasti būsimus apdovanojimus"</ns0:text>
        </ns0:outputEntry>
        <ns0:outputEntry id="LiteralExpression_0p3ey6k">
